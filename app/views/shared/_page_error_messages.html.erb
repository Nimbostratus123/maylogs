
<% if @page.errors.any? %>
<div id = 'error_log'>
	<p>This form contains <%= pluralize @page.errors.count, 'error' %> 			</p>
	
	<ul>
		<% @page.errors.full_messages.each do |error| %>
			<% unless error =~ /digest/i %>
				<li><span class = 'error_designation'>*</span>
					 <%= error%></li><br />
			<% end %>
		<% end %>
	</ul>
	
</div>
<% end %>
